  width: 300px;
  box-shadow: 0 2px 12px rgba(0, 0, 0, 0.1);
}

/* Gallery Section */
.gallery {
  padding: 60px 20px;
  text-align: center;
}

.gallery h2 {
  font-size: 2.5rem;
  margin-bottom: 40px;
}

.gallery-grid {
